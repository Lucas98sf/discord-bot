{
	"name": "botdisctest",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"dependencies": {
		"@koa/router": "^11.0.1",
		"axios": "^0.26.1",
		"discord.js": "^14.0.2",
		"koa": "^2.13.4",
		"koa-body": "^5.0.0",
		"koa-pino-logger": "^4.0.0",
		"mongoose": "^6.4.5",
		"twitter-api-v2": "^1.11.2"
	},
	"scripts": {
		"prepare": "husky install",
		"build": "npx rimraf ./dist && tsc",
		"start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
		"test": "jest --runInBand --coverage",
		"dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts"
	},
	"devDependencies": {
		"@types/jest": "^28.1.4",
		"@types/koa-pino-logger": "^3.0.1",
		"@types/koa__router": "^8.0.11",
		"@types/node": "^18.0.1",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.30.4",
		"@typescript-eslint/parser": "^5.30.4",
		"dotenv": "^16.0.1",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": ">=6",
		"jest": "^28.1.2",
		"lint-staged": ">=10",
		"pino-pretty": "^8.1.0",
		"prettier": "^2.7.1",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.5",
		"ts-node": "^10.8.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.4"
	},
	"lint-staged": {
		"*.ts": [
			"eslint --cache --fix .",
			"prettier --write .",
			"yarn test --onlyChanged --passWithNoTests"
		],
		"*.{json,md}": "prettier --write ."
	}
}
